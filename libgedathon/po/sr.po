# Create a new attribute object
# Kreirajte novi objekat atributa
#: libgedathon/include/geda_module.h:99
msgid "Create a new attribute object."
msgstr "Креирајте нови објекат атрибута."

# New object type syntax
# Nova sintaksa tipa objekta
#: libgedathon/include/geda_module.h:102
