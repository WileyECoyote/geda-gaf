# Close all files and the library
#: libgedathon/include/geda_module.h:53
msgid "Close all files and deinitialize the library."
msgstr "تمام فایل ها و کتابخانه را ببندید."

#: libgedathon/include/geda_module.h:55
msgid "Append a folder to the library search path."
msgstr "یک پوشه را به مسیر جستجوی کتابخانه اضافه کنید."

#: libgedathon/include/geda_module.h:56
msgid "Creates a gafrc file in the current directory."
msgstr "یک فایل gafrc را در دایرکتوری فعلی ایجاد می کند."

# Retrieve list of all pages
#: libgedathon/include/geda_module.h:59
msgid "Retrieve list of all pages."
msgstr "لیست بازیابی تمام صفحات را بازیابی کنید."

#: libgedathon/include/geda_module.h:60
msgid "Get pointer to the active page."
msgstr "اشاره گر به صفحه فعال را دریافت کنید."

# Set the active page
#: libgedathon/include/geda_module.h:61
msgid "Set the active page."
msgstr "صفحه فعال را تنظیم کنید."

#: libgedathon/include/geda_module.h:62
msgid "Returns True if page has been modified and not saved."
msgstr "درست می شود اگر صفحه تغییر کرده و ذخیره نشده باشد."

#: libgedathon/include/geda_module.h:63
msgid "Set the active page and change the current directory."
msgstr "صفحه فعال را تنظیم کنید دایرکتوری فعلی را تغییر دهید."

#: libgedathon/include/geda_module.h:64
msgid "Open a page."
msgstr "یک صفحه باز کن."

#: libgedathon/include/geda_module.h:65
msgid "Close page."
msgstr "صفحه بستن."

#: libgedathon/include/geda_module.h:66
msgid "Create a new empty page."
msgstr "یک صفحه خالی جدید ایجاد کنید."

#: libgedathon/include/geda_module.h:67
msgid "Rename a page changes the filename."
msgstr "تغییر نام یک صفحه نام فایل را تغییر می دهد."

#: libgedathon/include/geda_module.h:68
msgid "Save the page by writing to storage."
msgstr "با نوشتن به ذخیره سازی، صفحه را ذخیره کنید."

#: libgedathon/include/geda_module.h:69
msgid "Save the page using a new file name."
msgstr "صفحه را با استفاده از یک نام فایل جدید ذخیره کنید."

#: libgedathon/include/geda_module.h:70
msgid "Save all opened pages to storage."
msgstr "تمام صفحات باز شده را به ذخیره سازی ذخیره کنید."

#: libgedathon/include/geda_module.h:73
msgid "Returns True if object is a GedaCapsule, otherwise False."
msgstr "بازده درست است شی است GedaCapsule، در غیر این صورت کاذب است."

#: libgedathon/include/geda_module.h:74
msgid "Returns the rectangular boundaries of an object."
msgstr "مرزهای مستطیلی یک شی را باز می گرداند."

#: libgedathon/include/geda_module.h:75
msgid "Retrieve an object from a GedaCapsule."
msgstr "یک شی از یک را بازیابی کنید GedaCapsule."

#: libgedathon/include/geda_module.h:76
msgid "Retrieve all objects belonging to a page."
msgstr "یک شی را به یک صفحه یا یک شی دیگری اضافه کنید."

