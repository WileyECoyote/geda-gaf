# Get or Set the default symbol for capacitors
# Otrymaty abo vstanovyty symvol za umovchannyam dlya kondensatoriv
#: libgedathon/include/functions.h:54
msgid "Get or Set the default symbol for capacitors\n"
msgstr "Отримати або встановити символ за умовчанням для конденсаторів\n"

# Get or Set the default symbol for electrolytic capacitors
# Otrymaty abo vstanovyty symvol za zamovchuvannyam dlya elektrolitychnykh kondensatoriv
#: libgedathon/include/functions.h:55
msgid "Get or Set the default symbol for electrolytic capacitors\n"
msgstr ""
"Отримати або встановити символ за замовчуванням для електролітичних "
"конденсаторів\n"

# Get or Set the default symbol for inductors
# Otrymaty abo Vstanovyty symvol za zamovchuvannyam dlya induktoriv
#: libgedathon/include/functions.h:56
msgid "Get or Set the default symbol for inductors\n"
msgstr "Отримати або Встановити символ за замовчуванням для індукторів\n"

# Get or Set the default operational amplifier symbol
# Otrymaty abo vstanovyty symvol operatsiynoho pidsylyuvacha za zamovchuvannyam
#: libgedathon/include/functions.h:57
msgid "Get or Set the default operational amplifier symbol\n"
msgstr ""
"Отримати або встановити символ операційного підсилювача за замовчуванням\n"

# Get or Set the default symbol for resistors
# Otrymaty abo Vstanovyty symvol za zamovchuvannyam dlya rezystoriv
#: libgedathon/include/functions.h:58
msgid "Get or Set the default symbol for resistors\n"
msgstr "Отримати або Встановити символ за замовчуванням для резисторів\n"

# Get or Set the default titleblock symbol
# Otrymaty abo vstanovyty symvol zaholovka za umovchannyam
#: libgedathon/include/functions.h:59
msgid "Get or Set the default titleblock symbol\n"
msgstr "Отримати або встановити символ заголовка за умовчанням\n"

# Close all files and deinitialize the library
# Zakryyte vsi fayly ta deinitsializuyte biblioteku
#: libgedathon/include/geda_module.h:53
msgid "Close all files and deinitialize the library."
msgstr "Закрийте всі файли та деініціалізуйте бібліотеку."

# Append a folder to the library search path
# Dodayte papku do shlyakhu poshuku biblioteky
#: libgedathon/include/geda_module.h:55
msgid "Append a folder to the library search path."
msgstr "Додайте папку до шляху пошуку бібліотеки."

# Creates a gafrc file in the current directory
# Stvoryuye fayl gafrc u potochnomu katalozi
#: libgedathon/include/geda_module.h:56
msgid "Creates a gafrc file in the current directory."
msgstr "Створює файл gafrc у поточному каталозі."

# Retrieve list of all pages
# Otrymaty spysok vsikh storinok
#: libgedathon/include/geda_module.h:59
msgid "Retrieve list of all pages."
msgstr "Отримати список всіх сторінок."

# Get pointer to the active page
# Otrymaty pokazhchyk na aktyvnu storinku
#: libgedathon/include/geda_module.h:60
msgid "Get pointer to the active page."
msgstr "Отримати покажчик на активну сторінку."

# Set the active page
# Vstanovyty aktyvnu storinku
#: libgedathon/include/geda_module.h:61
msgid "Set the active page."
msgstr "Встановити активну сторінку."

# Returns true if page has been modified and not saved
# Povertaye true, yakshcho storinka bula zminena i ne zberezhena
#: libgedathon/include/geda_module.h:62
msgid "Returns True if page has been modified and not saved."
msgstr "Повертає true, якщо сторінка була змінена і не збережена."

# Set the active page and changes current directory
# Vstanovitʹ aktyvnu storinku ta zminitʹ potochnyy kataloh
#: libgedathon/include/geda_module.h:63
msgid "Set the active page and change the current directory."
msgstr "Встановіть активну сторінку та змініть поточний каталог."

# Open a page
# Vidkryyte storinku
#: libgedathon/include/geda_module.h:64
msgid "Open a page."
msgstr "Відкрийте сторінку."

# Close page
# Zakryty storinku
#: libgedathon/include/geda_module.h:65
msgid "Close page."
msgstr "Закрити сторінку."

# Create a new empty page
# Stvoritʹ novu pustu storinku
#: libgedathon/include/geda_module.h:66
msgid "Create a new empty page."
msgstr "Створіть нову пусту сторінку."

# Rename a page changes the filename
# Pereymenuvannya storinky zminyuye nazvu faylu
#: libgedathon/include/geda_module.h:67
msgid "Rename a page changes the filename."
msgstr "Перейменування сторінки змінює назву файлу."

# Save the page by writing to storage
# Zberezhitʹ storinku, zapysavshy yiyi na skhovyshche
#: libgedathon/include/geda_module.h:68
msgid "Save the page by writing to storage."
msgstr "Збережіть сторінку, записавши її на сховище."

# Save the page using a new file name
# Zberezhitʹ storinku za dopomohoyu novoho imeni faylu
#: libgedathon/include/geda_module.h:69
msgid "Save the page using a new file name."
msgstr "Збережіть сторінку за допомогою нового імені файлу."

# Save all opened pages to storage
# Zberezhitʹ vsi vidkryti storinky do skhovyshcha
#: libgedathon/include/geda_module.h:70
msgid "Save all opened pages to storage."
msgstr "Збережіть всі відкриті сторінки до сховища."

# Returns True if object is a Geda container, otherwise False
# Povertaye Pravda, yakshcho ob'yekt ye konteynerom Geda, inakshe false
#: libgedathon/include/geda_module.h:73
msgid "Returns True if object is a GedaCapsule, otherwise False."
msgstr "Повертає Правда, якщо об'єкт є контейнером Geda, інакше false."

# Returns the rectangular boundaries of an object
# Povertaye pryamokutni mezhi ob'yekta
#: libgedathon/include/geda_module.h:74
msgid "Returns the rectangular boundaries of an object."
msgstr "Повертає прямокутні межі об'єкта."

# Retrieve an object from a Geda container
# Otrymaty ob'yekt z konteynera Geda
#: libgedathon/include/geda_module.h:75
msgid "Retrieve an object from a GedaCapsule."
msgstr "Отримати об'єкт з контейнера Geda."

# Retrieve all objects belonging to a page
# Otrymaty vsi ob'yekty, shcho nalezhatʹ storintsi
#: libgedathon/include/geda_module.h:76
msgid "Retrieve all objects belonging to a page."
msgstr "Отримати всі об'єкти, що належать сторінці."

# Add an object to a page or another object
# Dodayte ob'yekt do storinky chy inshoho ob'yekta
#: libgedathon/include/geda_module.h:77
msgid "Add an object to a page or another object."
msgstr "Додайте об'єкт до сторінки чи іншого об'єкта."

# Add multiple objects to a page or another object
# Dodavannya dekilʹkokh ob'yektiv na storinku abo inshyy ob'yekt
#: libgedathon/include/geda_module.h:78
msgid "Add multiple objects to a page or another object."
msgstr "Додавання декількох об'єктів на сторінку або інший об'єкт."

# Make a copy an object
# Zrobitʹ kopiyu ob'yekta
#: libgedathon/include/geda_module.h:79
msgid "Make a copy an object."
msgstr "Зробіть копію об'єкта."

# Remove object from a page
# Vydalyty ob'yekt zi storinky
#: libgedathon/include/geda_module.h:80
msgid "Remove object from a page."
msgstr "Видалити об'єкт зі сторінки."

# Remove multiple objects from a page
# Vydalyty kilʹka ob'yektiv zi storinky
#: libgedathon/include/geda_module.h:81
msgid "Remove multiple objects from a page."
msgstr "Видалити кілька об'єктів зі сторінки."

# Delete an object from a page
# Vydalyty ob'yekt zi storinky
#: libgedathon/include/geda_module.h:83
msgid "Delete an object from a page."
msgstr "Видалити об'єкт зі сторінки."

# Delete multiple objects from a page
# Vydalyty kilʹka ob'yektiv zi storinky
#: libgedathon/include/geda_module.h:84
msgid "Delete multiple objects from a page."
msgstr "Видалити кілька об'єктів зі сторінки."

# Synchronize object with library
#: libgedathon/include/geda_module.h:85
msgid "Synchronize object with library."
msgstr "Синхронізувати об'єкт з бібліотекою."

# Create a new arc object
# Stvoritʹ novyy ob'yekt duhy
#: libgedathon/include/geda_module.h:88
msgid "Create a new arc object."
msgstr "Створіть новий об'єкт дуги."

# Створіть новий об'єкт
#: libgedathon/include/geda_module.h:89
msgid "Create a new box object."
msgstr "Stvoritʹ novyy ob'yekt."

# Create a new bus object
# Stvoritʹ novyy ob'yekt shyny
#: libgedathon/include/geda_module.h:90
msgid "Create a new bus object."
