# Get or Set the default symbol for capacitors
# Huòqǔ huò shèzhì diànróngqì de mòrèn fúhào
#: libgedathon/include/functions.h:54
msgid "Get or Set the default symbol for capacitors\n"
msgstr "获取或设置电容器的默认符号\n"

# Get or Set the default symbol for electrolytic capacitors
# Huòqǔ huò shèzhì diànjiě diànróng de mòrèn fúhào
#: libgedathon/include/functions.h:55
msgid "Get or Set the default symbol for electrolytic capacitors\n"
msgstr "获取或设置电解电容的默认符号\n"

# Get or Set the default symbol for inductors
# Huòqǔ huò shèzhì diàngǎn de mòrèn fúhào
#: libgedathon/include/functions.h:56
msgid "Get or Set the default symbol for inductors\n"
msgstr "获取或设置电感的默认符号\n"

# Get or Set the default operational amplifier symbol
# Huòqǔ huò shèzhì mòrèn yùnsuàn fàngdàqì fúhào
#: libgedathon/include/functions.h:57
msgid "Get or Set the default operational amplifier symbol\n"
msgstr "获取或设置默认运算放大器符号\n"

# Get or Set the default symbol for resistors
# Huòqǔ huò shèzhì diànzǔ de mòrèn fúhào
#: libgedathon/include/functions.h:58
msgid "Get or Set the default symbol for resistors\n"
msgstr "获取或设置电阻的默认符号\n"

# Get or Set the default title block symbol
# Huòqǔ huò shèzhì mòrèn biāotí lán fúhào
#: libgedathon/include/functions.h:59
msgid "Get or Set the default titleblock symbol\n"
msgstr "获取或设置默认标题栏符号\n"

# Close all files and deinitialize the library
# Guānbì suǒyǒu wénjiàn bìng qǔxiāo chūshǐhuà kù
#: libgedathon/include/geda_module.h:53
msgid "Close all files and deinitialize the library."
msgstr "关闭所有文件并取消初始化库."

# Append a folder to the library search path
# Jiāng wénjiàn jiā fùjiā dào kù sōusuǒ lùjìng
#: libgedathon/include/geda_module.h:55
msgid "Append a folder to the library search path."
msgstr "将文件夹附加到库搜索路径."

# Creates a gafrc file in the current directory
# Zài dāngqián mùlù zhōng chuàngjiàn gafrc wénjiàn
#: libgedathon/include/geda_module.h:56
msgid "Creates a gafrc file in the current directory."
msgstr "在当前目录中创建gafrc文件."

# Retrieve list of all pages
# Jiǎnsuǒ suǒyǒu yèmiàn dì lièbiǎo
#: libgedathon/include/geda_module.h:59
msgid "Retrieve list of all pages."
msgstr "检索所有页面的列表."

# Get pointer to the active page
# Huòqǔ zhǐxiàng huódòng yèmiàn de zhǐzhē
#: libgedathon/include/geda_module.h:60
msgid "Get pointer to the active page."
msgstr "获取指向活动页面的指针."

# Set the active page
# Shèzhì huódòng yèmiàn
#: libgedathon/include/geda_module.h:61
msgid "Set the active page."
msgstr "设置活动页面."

# Returns true if page has been modified and not saved
# Rúguǒ yèmiàn yǐ bèi xiūgǎi dàn wèi bǎocún, zé fǎnhuí Zhēnzhèng
#: libgedathon/include/geda_module.h:62
msgid "Returns True if page has been modified and not saved."
msgstr "如果頁面已被修改但未保存，則返回真正."

# Set the active page, and changes current directory
# Shèzhì huódòng yèmiàn, bìng gēnggǎi dāngqián mùlù
#: libgedathon/include/geda_module.h:63
msgid "Set the active page, and changes current directory."
msgstr "设置活动页面，并更改当前目录."

# Open a page
# Dǎkāi yīgè yèmiàn
#: libgedathon/include/geda_module.h:64
msgid "Open a page."
msgstr "打开一个页面."

# Close page
# Guānbì yèmiàn
#: libgedathon/include/geda_module.h:65
msgid "Close page."
msgstr "关闭页面."

# Create a new empty page
# Chuàngjiàn yīgè xīn de kōng yèmiàn
#: libgedathon/include/geda_module.h:66
msgid "Create a new empty page."
msgstr "创建一个新的空页面."

# Rename a page changes the filename
# Zhòng mìngmíng yèmiàn huì gēnggǎi wénjiàn míng
#: libgedathon/include/geda_module.h:67
msgid "Rename a page changes the filename."
msgstr "重命名页面会更改文件名."

# Save the page by writing to storage
# Tōngguò xiě rù cúnchú lái bǎocún yèmiàn
#: libgedathon/include/geda_module.h:68
msgid "Save the page by writing to storage."
msgstr "通过写入存储来保存页面."

# Save the page using a new file name
# Shǐyòng xīn wénjiàn míng bǎocún yèmiàn
#: libgedathon/include/geda_module.h:69
msgid "Save the page using a new file name."
msgstr "使用新文件名保存页面."

# Save all opened pages to storage
# Jiāng suǒyǒu dǎkāi de yèmiàn bǎocún dào cúnchú
#: libgedathon/include/geda_module.h:70
msgid "Save all opened pages to storage."
msgstr "将所有打开的页面保存到存储."

# Returns true if object is a Geda container, otherwise false
# Rúguǒ Bīnyǔ shì Geda róngqì, zé fǎnhuí Zhēnzhèng, fǒuzé fǎnhuí Jiǎ
#: libgedathon/include/geda_module.h:73
msgid "Returns True if object is a GedaCapsule, otherwise False."
msgstr "如果宾语是Geda容器，则返回真正，否则返回假."

# Returns the rectangular boundaries of an object
# Fǎnhuí duìxiàng de jǔxíng biānjiè
#: libgedathon/include/geda_module.h:74
msgid "Returns the rectangular boundaries of an object."
msgstr "返回对象的矩形边界."

# Retrieve an object from a Geda container
# Cóng Geda róngqì zhōng jiǎnsuǒ duìxiàng
#: libgedathon/include/geda_module.h:75
msgid "Retrieve an object from a GedaCapsule."
msgstr "从Geda容器中检索对象."

# Retrieve all objects belonging to a page
# Jiǎnsuǒ shǔyú yèmiàn de suǒyǒu duìxiàng
#: libgedathon/include/geda_module.h:76
msgid "Retrieve all objects belonging to a page."
msgstr "Jiǎnsuǒ shǔyú yèmiàn de suǒyǒu duìxiàng."

# Add an object to a page or another object
# Jiāng duìxiàng tiānjiā dào yèmiàn huò qítā duìxiàng
#: libgedathon/include/geda_module.h:77
msgid "Add an object to a page or another object."
msgstr "将对象添加到页面或其他对象."

# Add multiple objects to a page or another object
# Jiāng duō gè duìxiàng tiānjiā dào yèmiàn huò lìng yīgè duìxiàng
#: libgedathon/include/geda_module.h:78
msgid "Add multiple objects to a page or another object."
msgstr "将多个对象添加到页面或另一个对象."

# Make a copy an object
# Fùzhì yì gè duìxiàng
#: libgedathon/include/geda_module.h:79
msgid "Make a copy an object."
msgstr "复制一个对象."

# Remove object from a page
# Cóng yèmiàn zhōng shānchú duìxiàng
#: libgedathon/include/geda_module.h:80 libgedathon/include/geda_module.h:85
msgid "Remove object from a page."
msgstr "从页面中删除对象."

# Remove multiple objects from a page
# Cóng yèmiàn zhōng shānchú duō gè duìxiàng
#: libgedathon/include/geda_module.h:81
msgid "Remove multiple objects from a page."
msgstr "从页面中删除多个对象."

# Delete an object from a page
# Cóng yèmiàn zhōng shānchú duìxiàng
#: libgedathon/include/geda_module.h:83
msgid "Delete an object from a page."
msgstr "从页面中删除对象."

# Delete multiple objects from a page
# Cóng yèmiàn zhōng shānchú duō gè duìxiàng
#: libgedathon/include/geda_module.h:84
msgid "Delete multiple objects from a page."
msgstr "从页面中删除多个对象."

