# Select this Object
# Επιλέξτε αυτό το αντικείμενο
#: libgedathon/include/geda_module.h:107
msgid "Select this Object"
msgstr "Επιλέξτε αυτό το αντικείμενο"

# Unselect this Object
# Katargíste tin epilogí aftoú tou antikeiménou
#: libgedathon/include/geda_module.h:108
