# Convenience stub makefile to call the real Makefile.

@SET_MAKE@

OBJEXT    = l@OBJEXT@
SubFolder = object

# Explicit so that it's the default rule.
all:
	cd .. && $(MAKE) $(SubFolder)/all

clean %.a %.$(OBJEXT):
	cd .. && $(MAKE) $(SubFolder)/$@

.PHONY: all clean

.SUFFIXES:
.SUFFIXES: .a .$(OBJEXT)
